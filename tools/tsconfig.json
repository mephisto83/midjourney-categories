{
  "compilerOptions": {

    "moduleResolution": "Node",
    "module": "CommonJS",
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "outDir": "lib",
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "target": "ES2022",
  },
  "compileOnSave": true,
  "include": [
    "src"
  ],
  "paths": {
    "*": [
      "node_modules/*"
    ]
  }
}